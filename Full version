function toggleDrawMode(){isDrawing=!isDrawing;isRedrawing=false;const btn=document.getElementById('drawBtn');const c=document.getElementById('drawControls');if(isDrawing){btn.textContent="Ø¬Ø§Ø±ÙŠ Ø§Ù„Ø±Ø³Ù…...";btn.classList.add('active');c.style.display='block';document.body.classList.add('drawing-mode');drawPoints=[];drawnParts=[];initTempPath();updateSymmetryAxis();document.querySelectorAll('.muscle-zone').forEach(z=>z.classList.remove('editing'));}else{cancelDrawing();}}
function startRedraw(){if(!currentId)return;isDrawing=true;isRedrawing=true;document.getElementById('redrawBtn').textContent="Ø¬Ø§Ø±ÙŠ Ø§Ù„ØªØ¹Ø¯ÙŠÙ„...";document.getElementById('redrawBtn').classList.add('active');document.getElementById('drawControls').style.display='block';document.body.classList.add('drawing-mode');drawPoints=[];drawnParts=[];initTempPath();document.querySelectorAll('.muscle-zone').forEach(z=>z.classList.remove('editing'));} 
function initTempPath(){const s=document.getElementById('svgOverlay');document.getElementById('tempDrawPath')?.remove();document.getElementById('tempFinishedPath')?.remove();const tp=document.createElementNS("http://www.w3.org/2000/svg","path");tp.id='tempDrawPath';tp.classList.add('temp-path');s.appendChild(tp);const tf=document.createElementNS("http://www.w3.org/2000/svg","path");tf.id='tempFinishedPath';tf.classList.add('temp-path-finished');s.appendChild(tf);}
function handleMapClick(e){if(!isDrawing)return;const r=document.getElementById('svgOverlay').getBoundingClientRect();const x=Math.round((e.clientX-r.left)*(1000/r.width));const y=Math.round((e.clientY-r.top)*(667/r.height));drawPoints.push([x,y]);updateTempPath();e.stopPropagation();}
function updateTempPath(){const el=document.getElementById('tempDrawPath');if(!el)return;if(drawPoints.length===0){el.setAttribute("d","");return;}const d=drawPoints.map((p,i)=>(i===0?"M":"L")+p[0]+","+p[1]).join(" ");el.setAttribute("d",d);}
function addManualPart(){if(drawPoints.length<3)return;const d=drawPoints.map((p,i)=>(i===0?"M":"L")+p[0]+","+p[1]).join(" ")+" Z";drawnParts.push(d);document.getElementById('tempFinishedPath').setAttribute("d",drawnParts.join(" "));drawPoints=[];updateTempPath();}
function undoLastPoint(){if(drawPoints.length>0)drawPoints.pop();else if(drawnParts.length>0){drawnParts.pop();document.getElementById('tempFinishedPath').setAttribute("d",drawnParts.join(" "));}updateTempPath();}
function cancelDrawing(){isDrawing=false;isRedrawing=false;document.getElementById('drawBtn').textContent="+ Ø±Ø³Ù… Ø¹Ø¶Ù„Ø© Ø¬Ø¯ÙŠØ¯Ø©";document.getElementById('drawBtn').classList.remove('active');document.getElementById('redrawBtn').textContent="âœ Ø¥Ø¹Ø§Ø¯Ø© Ø±Ø³Ù…";document.getElementById('redrawBtn').classList.remove('active');document.getElementById('drawControls').style.display='none';document.body.classList.remove('drawing-mode');document.getElementById('tempDrawPath')?.remove();document.getElementById('tempFinishedPath')?.remove();drawPoints=[];drawnParts=[];if(!isPreview&Â¤tId)document.getElementById(currentId)?.classList.add('editing');}
function finishDrawing(sym){if(drawPoints.length>2)addManualPart();if(drawnParts.length===0)return;if(sym){const axis=parseFloat(document.getElementById('symmetryAxis').value);const lp=drawnParts[drawnParts.length-1].match(/[ML](\d+,\d+)/g).map(s=>s.substring(1).split(',').map(Number));const mp=lp.map(p=>[(2*axis)-p[0],p[1]]);drawnParts.push(mp.map((p,i)=>(i===0?"M":"L")+p[0]+","+p[1]).join(" ")+" Z");}const fd=drawnParts.join(" ");if(isRedrawing){defaultData[currentId].pathD=fd;save();restoreCustomMuscles();loadMuscleData(currentId);}else{const n=prompt("Ø§Ø³Ù… Ø§Ù„Ø¹Ø¶Ù„Ø© (Ø¥Ù†Ø¬Ù„ÙŠØ²ÙŠ):");if(n&&!defaultData[n]){defaultData[n]={title:"Ø¬Ø¯ÙŠØ¯",exercises:[],t:{x:0,y:0,sx:1,sy:1},pathD:fd};save();restoreCustomMuscles();refreshMuscleList();loadMuscleData(n);}}cancelDrawing();}
