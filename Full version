const LS_KEY = "CyberGym_Gamified";
let currentId = "chest", isPreview = true, isDrawing = false, isRedrawing = false, drawPoints = [], drawnParts = []; 
let saveTimer = null;

const audioFiles = {
    open: new Audio('https://assets.mixkit.co/active_storage/sfx/2573/2573-preview.mp3'),
    click: new Audio('https://assets.mixkit.co/active_storage/sfx/2568/2568-preview.mp3'),
    close: new Audio('https://assets.mixkit.co/active_storage/sfx/2572/2572-preview.mp3')
};
Object.values(audioFiles).forEach(a=>a.volume=0.4);
function playSfx(t){if(audioFiles[t]){audioFiles[t].currentTime=0;audioFiles[t].play().catch(()=>{})}}

function toggleTheme() {
    document.body.classList.toggle('theme-medical');
    const isMedical = document.body.classList.contains('theme-medical');
    const btn = document.querySelector('.theme-btn');
    if (btn) btn.innerHTML = isMedical ? 'âœš' : 'ğŸ§¬';
    localStorage.setItem('CyberGym_Theme_Game', isMedical ? 'medical' : 'cyber');
    playSfx('click');
}
function loadTheme() {
    const savedTheme = localStorage.getItem('CyberGym_Theme_Game');
    if (savedTheme === 'medical') {
        document.body.classList.add('theme-medical');
        const btn = document.querySelector('.theme-btn');
        if (btn) btn.innerHTML = 'âœš';
    }
}

function init(){
    loadTheme();
    const saved = localStorage.getItem(LS_KEY);
    if(saved){try{const p=JSON.parse(saved);if(p.config)globalConfig=p.config;if(p.data)defaultData={...defaultData,...p.data};}catch(e){console.error(e);}}
    
    document.getElementById('mainImageInput').value = globalConfig.mainImage;
    document.getElementById('mainAnatomyImage').src = globalConfig.mainImage;
    
    // ğŸ’¡ Ø§Ù„ØªØ­Ø³ÙŠÙ†: Ø¬Ø¯ÙˆÙ„Ø© Ø§Ù„Ø¹Ù…Ù„ÙŠØ§Øª Ø§Ù„Ø«Ù‚ÙŠÙ„Ø© (Ù…Ø«Ù„ Ø±Ø³Ù… SVG) Ø¨Ø´ÙƒÙ„ ØºÙŠØ± Ù…ØªØ²Ø§Ù…Ù†
    requestAnimationFrame(() => {
        restoreCustomMuscles(); 
        refreshMuscleList();

        ['inpX','inpY','inpScaleX','inpScaleY'].forEach(id=>document.getElementById(id).addEventListener('input',updateLayout));
        document.getElementById('editTitle').addEventListener('input',e=>{if(defaultData[currentId]){defaultData[currentId].title=e.target.value;save();const sel=document.getElementById('muscleSelector');const opt=sel.querySelector(`option[value="${currentId}"]`);if(opt)opt.text=e.target.value;}});
        document.getElementById('editMainImg').addEventListener('input',e=>{if(defaultData[currentId]){defaultData[currentId].mainImg=e.target.value;save();}});
        document.getElementById('mainImageInput').addEventListener('input',e=>{globalConfig.mainImage=e.target.value;document.getElementById('mainAnatomyImage').src=e.target.value;save();});
        
        document.getElementById('svgOverlay').addEventListener('click', handleMapClick); 
        document.getElementById('svgOverlay').addEventListener('dblclick', ()=>finishDrawing(false));
        makePanelDraggable();
    });
}

function restoreCustomMuscles() {
    const svgOverlay = document.getElementById('svgOverlay');
    svgOverlay.innerHTML = ''; 
    const muscleKeys = Object.keys(defaultData).filter(key => defaultData[key].pathD);
    let index = 0;
    
    // ğŸ’¡ Ø§Ù„ØªØ­Ø³ÙŠÙ†: ØªÙ‚Ø³ÙŠÙ… Ø§Ù„Ø¹Ù…Ù„ Ø¹Ø¨Ø± Ø¯Ø§Ù„Ø© ØªÙƒØ±Ø§Ø±ÙŠØ© ÙˆØ¬Ø¯ÙˆÙ„Ø© Ø²Ù…Ù†ÙŠØ©
    function processNextMuscle() {
        if (index >= muscleKeys.length) return; 

        const key = muscleKeys[index];
        const data = defaultData[key];
        
        // ğŸ’¡ Ø§Ù„ØªØ­Ø³ÙŠÙ†: Ø§Ø³ØªØ®Ø¯Ø§Ù… try/catch Ù„ØªØ¬Ù†Ø¨ ÙØ´Ù„ Ø§Ù„Ø¹Ù…Ù„ÙŠØ© Ø¨Ø§Ù„ÙƒØ§Ù…Ù„
        try {
             const newPath = document.createElementNS("http://www.w3.org/2000/svg", "path");
             newPath.setAttribute("id", key); newPath.setAttribute("class", "muscle-zone"); newPath.setAttribute("d", data.pathD);
             svgOverlay.appendChild(newPath);
             updateDOMTransform(key);
             newPath.addEventListener('click', e=>{ 
                 if(isDrawing)return; 
                 if(isPreview){
                     // ÙŠØ¬Ø¨ Ø£Ù† ÙŠØªÙ… ØªÙØ¹ÙŠÙ„ Ø§Ù„Ù…Ù†Ø·Ù‚Ø© Ø¨Ø´ÙƒÙ„ Ø³Ø±ÙŠØ¹
                     activateZone(newPath);
                     e.stopPropagation();
                 } else {
                     loadMuscleData(newPath.id);
                     toggleAdmin(true);
                     e.stopPropagation();
                 }
             });
        } catch(e) {
            console.error(`Error restoring muscle ${key}:`, e);
        }
        
        index++;
        // Ø§Ø³ØªØ®Ø¯Ø§Ù… setTimeout(..., 0) Ù„Ø¬Ø¯ÙˆÙ„Ø© Ø§Ù„Ø¹Ù…Ù„ÙŠØ© Ø§Ù„ØªØ§Ù„ÙŠØ© ÙÙŠ Ø­Ù„Ù‚Ø© Ø§Ù„Ø£Ø­Ø¯Ø§Ø« (Event Loop)
        // Ù…Ù…Ø§ ÙŠØ³Ù…Ø­ Ù„Ù„Ù…ØªØµÙØ­ Ø¨ØªØ­Ø¯ÙŠØ« ÙˆØ§Ø¬Ù‡Ø© Ø§Ù„Ù…Ø³ØªØ®Ø¯Ù… Ø¨ÙŠÙ† Ø§Ù„Ø¹Ù…Ù„ÙŠØ§Øª
        setTimeout(processNextMuscle, 0); 
    }
    
    // Ø¨Ø¯Ø¡ Ø§Ù„Ù…Ø¹Ø§Ù„Ø¬Ø©
    processNextMuscle();
}


function refreshMuscleList() {
    const sel = document.getElementById('muscleSelector'); sel.innerHTML='';
    const muscleKeys = Object.keys(defaultData).filter(key => defaultData[key].pathD);
    if(muscleKeys.length === 0) { sel.innerHTML = '<option>Ù„Ø§ ØªÙˆØ¬Ø¯ Ø¨ÙŠØ§Ù†Ø§Øª</option>'; currentId = null; return; }
    muscleKeys.forEach(key => { const opt=document.createElement('option'); opt.value=key; opt.text=defaultData[key].title||key; sel.appendChild(opt); });
    if(muscleKeys.includes(currentId)) sel.value = currentId; else if(muscleKeys.length > 0) { currentId = muscleKeys[0]; sel.value = currentId; }
}

function activateZone(zone){
    playSfx('open');
    document.querySelectorAll('.muscle-zone').forEach(z=>z.classList.remove('active')); zone.classList.add('active');
    
    let bbox; 
    // ğŸ’¡ Ø§Ù„ØªØ­Ø³ÙŠÙ†: ØªØ¬Ø±Ø¨Ø© Ø§Ù„Ø­ØµÙˆÙ„ Ø¹Ù„Ù‰ getBBox Ù„ØªØ¬Ù†Ø¨ Ø§Ù„ÙØ´Ù„ ÙÙŠ Ø¨Ø¹Ø¶ Ø§Ù„Ù…ØªØµÙØ­Ø§Øª
    try { 
        bbox = zone.getBBox(); 
    } catch (e) { 
        bbox = { x: 500, y: 333.5, width: 0, height: 0 }; 
    }
    const cx = bbox.x + bbox.width/2, cy = bbox.y + bbox.height/2;
    document.getElementById('zoomLayer').style.transformOrigin=`${(cx/1000)*100}% ${(cy/667)*100}%`; 
    document.getElementById('zoomLayer').style.transform="scale(2.3)";
    
    const data=defaultData[zone.id];
    document.getElementById('cardTitle').textContent=data.title;
    const img=document.getElementById('mainMuscleImg'); img.src=data.mainImg||''; img.style.display=data.mainImg?'block':'none';
    const bc=document.getElementById('buttonsContainer'); bc.innerHTML='';
    if(data.exercises&&data.exercises.length>0){data.exercises.forEach(ex=>{const b=document.createElement('div');b.className='exercise-btn';b.textContent=ex.name;b.onclick=()=>showDetail(ex);bc.appendChild(b);});}
    else bc.innerHTML='<p style="text-align:center;color:#888;">Ù„Ø§ ØªÙˆØ¬Ø¯ ØªÙ…Ø§Ø±ÙŠÙ†</p>';
    showListView();
    // ğŸ’¡ Ø§Ù„ØªØ­Ø³ÙŠÙ†: Ø§Ø³ØªØ®Ø¯Ø§Ù… setTimeout Ù„Ù„Ø³Ù…Ø§Ø­ Ø¨ØªØ­Ø±ÙŠÙƒ Ø§Ù„Ù€ zoom Ù‚Ø¨Ù„ ÙØªØ­ Ø§Ù„Ø¨Ø·Ø§Ù‚Ø©
    setTimeout(()=>{document.getElementById('backdrop').classList.add('show');document.getElementById('infoCard').classList.add('show');},100);
}

function showDetail(ex){
    playSfx('click');
    document.getElementById('detailTitle').textContent=ex.name;
    document.getElementById('detailDesc').innerHTML=(ex.desc||"").replace(/\n/g, '<br>');
    document.getElementById('detailImgStart').src=ex.images?.startImg||'https://placehold.co/600x400/222/777?text=No+Img';
    document.getElementById('detailImgStart').style.display='block';
    document.getElementById('detailImgEnd').src=ex.images?.endImg||'';
    document.getElementById('detailImgEnd').style.display=ex.images?.endImg?'block':'none';
    document.getElementById('viewList').classList.remove('active'); document.getElementById('viewDetail').classList.add('active');
}
function showListView(){playSfx('click');document.getElementById('viewDetail').classList.remove('active');document.getElementById('viewList').classList.add('active');}
function resetZoom(){playSfx('close');document.getElementById('infoCard').classList.remove('show');document.getElementById('backdrop').classList.remove('show');document.getElementById('zoomLayer').style.transform="scale(1)";setTimeout(()=>{document.querySelectorAll('.muscle-zone').forEach(z=>z.classList.remove('active'));},300);}

function updateDOMTransform(id){const el=document.getElementById(id);if(el&&defaultData[id]?.t){const t=defaultData[id].t;el.style.transform=`translate(${t.x}px,${t.y}px) scale(${t.sx},${t.sy})`;}}
function toggleAdmin(show){document.getElementById('adminPanel').style.display=show?'flex':'none';if(show&Â¤tId)loadMuscleData(currentId);}
function switchTab(n){document.querySelectorAll('.panel-body').forEach(b=>b.classList.remove('active'));document.querySelectorAll('.tab-btn').forEach(b=>b.classList.remove('active'));document.getElementById('tab-'+n).classList.add('active');event.target.classList.add('active');}
function loadMuscleData(id){currentId=id;document.getElementById('muscleSelector').value=id;document.querySelectorAll('.muscle-zone').forEach(z=>z.classList.remove('editing'));const el=document.getElementById(id);if(el)el.classList.add('editing');if(!defaultData[id])return;const d=defaultData[id];document.getElementById('inpX').value=d.t.x;document.getElementById('inpY').value=d.t.y;document.getElementById('inpScaleX').value=d.t.sx;document.getElementById('inpScaleY').value=d.t.sy;document.getElementById('editTitle').value=d.title;document.getElementById('editMainImg').value=d.mainImg||'';document.getElementById('pathDataInput').value=d.pathD;renderExercises(d.exercises||[]);}
function renderExercises(list){const c=document.getElementById('exercisesContainer');c.innerHTML='';list.forEach((ex,i)=>{const div=document.createElement('div');div.className='exercise-block';div.innerHTML=`<button class="btn-del" onclick="deleteExercise(${i})">Ø­Ø°Ù</button><div class="control-group"><label>Ø§Ø³Ù… Ø§Ù„ØªÙ…Ø±ÙŠÙ†</label><input type="text" value="${ex.name}" oninput="updateEx(${i},'name',this.value)"></div><div class="control-group"><label>ØµÙˆØ±Ø©/GIF</label><input type="text" value="${ex.images?.startImg||''}" oninput="updateExImage(${i},'startImg',this.value)"></div><div class="control-group"><label>Ø§Ù„ÙˆØµÙ</label><textarea rows="3" oninput="updateEx(${i},'desc',this.value)">${ex.desc||''}</textarea></div>`;c.appendChild(div);});}
window.updateEx=(i,k,v)=>{if(defaultData[currentId]?.exercises[i]){defaultData[currentId].exercises[i][k]=v;save();}}
window.updateExImage=(i,k,v)=>{if(defaultData[currentId]?.exercises[i]){if(!defaultData[currentId].exercises[i].images)defaultData[currentId].exercises[i].images={};defaultData[currentId].exercises[i].images[k]=v;save();}}
window.addExercise=()=>{if(defaultData[currentId]){defaultData[currentId].exercises.push({name:"ØªÙ…Ø±ÙŠÙ† Ø¬Ø¯ÙŠØ¯",desc:"",images:{startImg:"",endImg:""}});renderExercises(defaultData[currentId].exercises);save();}}
window.deleteExercise=i=>{if(defaultData[currentId]){defaultData[currentId].exercises.splice(i,1);renderExercises(defaultData[currentId].exercises);save();}}
function updateLayout(){const t=defaultData[currentId].t;t.x=document.getElementById('inpX').value;t.y=document.getElementById('inpY').value;t.sx=document.getElementById('inpScaleX').value;t.sy=document.getElementById('inpScaleY').value;updateDOMTransform(currentId);save();}
function save(){
    clearTimeout(saveTimer);
    saveTimer = setTimeout(() => {
        try{
            localStorage.setItem(LS_KEY,JSON.stringify({config:globalConfig,data:defaultData}));
            document.getElementById('saveStatus').style.display='block';
            setTimeout(()=>{document.getElementById('saveStatus').style.display='none';},1200);
        }catch(e){console.error("Save Error:", e);}
    }, 500); 
}
function exportDataJSON(){const b=new Blob([JSON.stringify({config:globalConfig,data:defaultData},null,2)],{type:'application/json'});const a=document.createElement('a');a.href=URL.createObjectURL(b);a.download='CyberGym_Data.json';a.click();}
window.handleImport=f=>{const r=new FileReader();r.onload=e=>{try{const j=JSON.parse(e.target.result);globalConfig=j.config;defaultData=j.data;save();restoreCustomMuscles();alert('ØªÙ… Ø§Ù„Ø§Ø³ØªÙŠØ±Ø§Ø¯');location.reload();}catch(err){alert('Ø®Ø·Ø£');}};r.readAsText(f.files[0]);}
